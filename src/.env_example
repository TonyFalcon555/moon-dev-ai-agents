# Restream API Configuration
RESTREAM_CLIENT_ID=your_client_id_here
RESTREAM_CLIENT_SECRET=your_client_secret_here 

# =============================
# Moon Dev Core API & Data
# =============================

# Moon Dev upstream data API key (used by src/agents/api.py)
MOONDEV_API_KEY=your_moondev_api_key_here
MOONDEV_API_BASE_URL=http://api.moondev.com:8000

# =============================
# API Gateway (src/services/api_gateway)
# =============================

# Where the gateway proxies to for /files/* and copybot endpoints
UPSTREAM_API_BASE_URL=http://api.moondev.com:8000
# Optional upstream key; if set, forwarded as X-API-Key to upstream
UPSTREAM_API_KEY=

# Enable SQLite keystore (recommended)
USE_KEYSTORE=1
# Paths inside the gateway container or local env
KEYSTORE_DB_PATH=src/services/api_gateway/keystore.sqlite3
USAGE_DB_PATH=src/services/api_gateway/usage.sqlite3

# Fallback static keys when keystore disabled (format key:plan,key2:plan2)
API_KEYS=demo-key:free

# Optional per-plan rate limits (used by billing when provisioning keys)
RATE_LIMIT_PRO=600
RATE_LIMIT_TEAM=2400
RATE_LIMIT_ENTERPRISE=10000

# =============================
# Billing Service (src/services/billing)
# =============================

STRIPE_SECRET_KEY=sk_test_your_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Stripe price IDs per plan
PRICE_ID_PRO=price_pro_here
PRICE_ID_TEAM=price_team_here
PRICE_ID_ENTERPRISE=price_enterprise_here

# =============================
# Alerts Service (src/services/alerts)
# =============================

# Discord webhook where alerts are posted
ALERTS_DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your_webhook_here

# Polling & per-plan limits (optional overrides)
ALERTS_POLL_INTERVAL=60
ALERTS_MAX_FREE=3
ALERTS_MAX_PRO=20
ALERTS_MAX_TEAM=50
ALERTS_MAX_ENTERPRISE=200

# =============================
# Licensing & Feature Gating (src/licensing.py)
# =============================

# Licensing key and gateway URL used by Focus/Phone/Video/Clips agents
LICENSE_API_KEY=
LICENSE_GATEWAY_URL=http://127.0.0.1:8010

# If set to 1/true/yes, agents exit when license checks fail
REQUIRE_LICENSE=0

# Minimum plan required per feature
LICENSE_MIN_PLAN_FOCUS=free
LICENSE_MIN_PLAN_PHONE=free
LICENSE_MIN_PLAN_VIDEO=free
LICENSE_MIN_PLAN_CLIPS=free

# =============================
# RBI / Backtest Dashboard
# =============================

# Optional: default workspace name when running RBI agent directly
RBI_WORKSPACE_NAME=default

# =============================
# Exchanges / CopyBot / Aster (optional)
# =============================

